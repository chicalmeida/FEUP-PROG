#include <iostream>
#include <cmath>

//only 2.7a done

using namespace std;

bool isPrime(int n){
    for(int i =2; i<sqrt(n); i++){
        if(n%i==0){
            return false;
        }
        else{
            return true;
        }
    }
}

int main() {
    int n;
    cout<<"Enter an integer: "<<endl;
    cin>>n;
    if(isPrime(n)){
        cout << "Your number is prime"<<endl;
    }
    else{
        cout<<"Your number isn't prime"<<endl;
    }


}